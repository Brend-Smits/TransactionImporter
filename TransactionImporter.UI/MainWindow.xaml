<Window x:Class="TransactionImporter.UI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TransactionImporter.UI"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="535.598">
    <Grid>
        <Button x:Name="btnUploadFile" Content="Upload FIle" HorizontalAlignment="Left" Margin="30,240,0,0" VerticalAlignment="Top" Width="150" Height="53" Click="btnUploadFile_Click"/>
        <Button x:Name="btnRetrieveData" Content="Retrieve Data" HorizontalAlignment="Left" Margin="19,27,0,0" VerticalAlignment="Top" Width="144" Height="55" Click="btnRetrieveData_Click"/>
        <ListBox x:Name="lbTransactions" HorizontalAlignment="Left" Height="198" Margin="189,27,0,0" VerticalAlignment="Top" Width="321"/>
        <Button x:Name="btnRefresh" Content="Refresh Listbox" HorizontalAlignment="Left" Margin="314,240,0,0" VerticalAlignment="Top" Width="196" Height="53" Click="btnRefresh_Click"/>

    </Grid>
</Window>
